VITE_PRAGMA_EMAIL=test.mail@pragmago.pl
VITE_PRAGMA_PHONE_PREFIX=+48
VITE_PRAGMA_PHONE_NUMBER=45512444
VITE_PRAGMA_CHECKSUM="1234"

