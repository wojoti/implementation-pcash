# Klucz partnera z Strefy Partnera PragmaGO
VITE_PRAGMA_PARTNER_KEY=xxx

# ID klienta partnera
VITE_PRAGMA_CUSTOMER_ID=xxx

# URL powrotu po zakończeniu procesu
VITE_PRAGMA_RETURN_URL=https://twoja-domena.pl

# Dane klienta (opcjonalne)
VITE_PRAGMA_USER_ID=32145
VITE_PRAGMA_FIRST_NAME=Test
